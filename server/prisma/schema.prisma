generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type RecipesSteps {
  id   String @map("_id") @db.ObjectId
  body String
}

model recipes {
  id    String         @id @default(auto()) @map("_id") @db.ObjectId
  v     Int            @map("__v")
  steps RecipesSteps[]
  title String
}
